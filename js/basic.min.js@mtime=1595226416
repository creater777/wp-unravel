    return this.each(function () {
        var t = $(this);
        t.val() || t.val("rgba(255,255,255)").blur(), t.colorPicker({cssAddon: ".cp-color-picker { z-index: 1000; }"}).css({
            boxSizing: "border-box",
            marginTop: 8,
            height: t.outerHeight() - 8,
            paddingLeft: 10
        }).blur(function () {
            $(this).change()
        }), t.attr("id") && $('label[for="' + t.attr("id") + '"]').addClass("active");
        var e = t.prev();
        0 < e.length && "LABEL" == e.get(0).tagName && e.addClass("active")
    })
}, $(document).bind("dofirst", function (t, e) {
    $(e).find("input.color-picker").installColorPicker()
}), $.pjaxBlacklist = ".no-pjax, [data-gallery], .collapsible-header, .dcore-expected-redirection, .dcore-body a", $.pjaxInstance = function () {
    return $("#pjax-wrapper").data("smoothState")
}, $.pjaxRedirect = function (t) {
    var e = $.pjaxInstance();
    e ? (e.clear(t), e.load(t)) : location.href == t ? location.reload() : location.href = t
};