            var s = jQuery.trim(a[o]);
            if (s.substring(0, t.length + 1) == t + "=") {
                n = decodeURIComponent(s.substring(t.length + 1));
                break
            }
        }
        return n
    }
    i = i || {}, null === e && (e = "", i.expires = -1);
    var r, d = "";
    i.expires && ("number" == typeof i.expires || i.expires.toUTCString) && ("number" == typeof i.expires ? (r = new Date).setTime(r.getTime() + 24 * i.expires * 60 * 60 * 1e3) : r = i.expires, d = "; expires=" + r.toUTCString());
